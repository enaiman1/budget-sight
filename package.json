{
  "name": "budget-sight",
  "version": "1.0.0",
  "description": "This is a mern stack boilerplate",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enaiman1/mern-passport-boilerplate.git"
  },
  "author": "Eric Naiman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/enaiman1/mern-passport-boilerplate/issues"
  },
  "homepage": "https://github.com/enaiman1/mern-passport-boilerplate#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "concurrently": "^5.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.26.0",
    "mongoose": "^5.9.16",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "plaid": "^5.1.0",
    "validator": "^13.0.0"
  },
  "devDependencies": {
    "nodemon": "^1.19.1"
  }
